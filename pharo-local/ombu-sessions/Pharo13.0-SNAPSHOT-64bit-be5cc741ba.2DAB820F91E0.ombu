			#annotations : IdentityDictionary {
				#isMeta : false,
				#className : #ZnServer
			},
			#name : #'acceptStrategy:',
			#protocol : #options,
			#sourceCode : 'acceptStrategy: strategy\r\t"Set the server accept strategy that controls how incoming connections are handled.\r\tIf set to #unlimited, any incoming connection is accepted, \r\tbut could later on be refused when maximumNumberOfConcurrentConnections is reached, \r\tin which case the connection is closed after sending a 503 Service Unavailable response.\r\tIf set to #limited, incoming connections are accepted as long as maximumNumberOfConcurrentConnections has not yet been reached, \r\tonce it does, incoming connections are no longer accepted and the server waits for existing connections to close.\r\tIn this case, 503 Service Unavailable responses are normally not sent."\r\r\tself assert: (#(limited unlimted) includes: strategy).\r\r\tself localOptions at: #acceptStrategy put: strategy',
			#stamp : '',
			#package : #Zinc-HTTP
		}
	}
}OmEntry {
