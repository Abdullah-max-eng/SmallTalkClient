     := (SQLITE_IOERR | (23<<8)).\r    SQLITE_IOERR_MMAP               := (SQLITE_IOERR | (24<<8)).\r    SQLITE_IOERR_GETTEMPPATH        := (SQLITE_IOERR | (25<<8)).\r    SQLITE_IOERR_CONVPATH           := (SQLITE_IOERR | (26<<8)).\r    SQLITE_IOERR_VNODE              := (SQLITE_IOERR | (27<<8)).\r    SQLITE_IOERR_AUTH               := (SQLITE_IOERR | (28<<8)).\r    SQLITE_IOERR_BEGIN_ATOMIC       := (SQLITE_IOERR | (29<<8)).\r    SQLITE_IOERR_COMMIT_ATOMIC      := (SQLITE_IOERR | (30<<8)).\r    SQLITE_IOERR_ROLLBACK_ATOMIC    := (SQLITE_IOERR | (31<<8)).\r    SQLITE_LOCKED_SHAREDCACHE       := (SQLITE_LOCKED |  (1<<8)).\r    SQLITE_LOCKED_VTAB              := (SQLITE_LOCKED |  (2<<8)).\r    SQLITE_BUSY_RECOVERY            := (SQLITE_BUSY   |  (1<<8)).\r    SQLITE_BUSY_SNAPSHOT            := (SQLITE_BUSY   |  (2<<8)).\r    SQLITE_CANTOPEN_NOTEMPDIR       := (SQLITE_CANTOPEN | (1<<8)).\r    SQLITE_CANTOPEN_ISDIR           := (SQLITE_CANTOPEN | (2<<8)).\r    SQLITE_CANTOPEN_FULLPATH        := (SQLITE_CANTOPEN | (3<<8)).\r    SQLITE_CANTOPEN_CONVPATH        := (SQLITE_CANTOPEN | (4<<8)).\r    SQLITE_CANTOPEN_DIRTYWAL        := (SQLITE_CANTOPEN | (5<<8)). "Not Used"\r    SQLITE_CORRUPT_VTAB             := (SQLITE_CORRUPT | (1<<8)).\r    SQLITE_CORRUPT_SEQUENCE         := (SQLITE_CORRUPT | (2<<8)).\r    SQLITE_READONLY_RECOVERY        := (SQLITE_READONLY | (1<<8)).\r    SQLITE_READONLY_CANTLOCK        := (SQLITE_READONLY | (2<<8)).\r    SQLITE_READONLY_ROLLBACK        := (SQLITE_READONLY | (3<<8)).\r    SQLITE_READONLY_DBMOVED         := (SQLITE_READONLY | (4<<8)).\r    SQLITE_READONLY_CANTINIT        := (SQLITE_READONLY | (5<<8)).\r    SQLITE_READONLY_DIRECTORY       := (SQLITE_READONLY | (6<<8)).\r    SQLITE_ABORT_ROLLBACK           := (SQLITE_ABORT | (2<<8)).\r    SQLITE_CONSTRAINT_CHECK         := (SQLITE_CONSTRAINT | (1<<8)).\r    SQLITE_CONSTRAINT_COMMITHOOK    := (SQLITE_CONSTRAINT | (2<<8)).\r    SQLITE_CONSTRAINT_FOREIGNKEY    := (SQLITE_CONSTRAINT | (3<<8)).\r    SQLITE_CONSTRAINT_FUNCTION      := (SQLITE_CONSTRAINT | (4<<8)).\r    SQLITE_CONSTRAINT_NOTNULL       := (SQLITE_CONSTRAINT | (5<<8)).\r    SQLITE_CONSTRAINT_PRIMARYKEY    := (SQLITE_CONSTRAINT | (6<<8)).\r    SQLITE_CONSTRAINT_TRIGGER       := (SQLITE_CONSTRAINT | (7<<8)).\r    SQLITE_CONSTRAINT_UNIQUE        := (SQLITE_CONSTRAINT | (8<<8)).\r    SQLITE_CONSTRAINT_VTAB          := (SQLITE_CONSTRAINT | (9<<8)).\r    SQLITE_CONSTRAINT_ROWID         := (SQLITE_CONSTRAINT |(10<<8)).\r    SQLITE_NOTICE_RECOVER_WAL       := (SQLITE_NOTICE | (1<<8)).\r    SQLITE_NOTICE_RECOVER_ROLLBACK  := (SQLITE_NOTICE | (2<<8)).\r    SQLITE_WARNING_AUTOINDEX        := (SQLITE_WARNING | (1<<8)).\r    SQLITE_AUTH_USER                := (SQLITE_AUTH | (1<<8)).\r    SQLITE_OK_LOAD_PERMANENTLY      := (SQLITE_OK | (1<<8))',
