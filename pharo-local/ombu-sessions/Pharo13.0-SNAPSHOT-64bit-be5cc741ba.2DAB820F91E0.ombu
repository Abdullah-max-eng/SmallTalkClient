	#content : EpMethodAddition {
		#method : RGMethodDefinition {
			#annotations : IdentityDictionary {
				#isMeta : true,
				#className : #'SQLite3Constants class'
			},
			#name : #initErrorCodes,
			#protocol : #'private - initialization constants',
			#sourceCode : 'initErrorCodes\r\t"In its default configuration, SQLite API routines return one of 26 integer\r\t[SQLITE_OK | result codes]."\r\r\tSQLITE_ERROR := 1. \t\t\t"SQL error or missing database"\r\tSQLITE_INTERNAL := 2.\t\t\t"Internal logic error in SQLite"\r\tSQLITE_PERM := 3.\t\t\t\t"Access permission denied"\r\tSQLITE_ABORT := 4.\t\t\t\t"Callback routine requested an abort"\r\tSQLITE_BUSY := 5.  \t\t\t"The database file is locked"\r\tSQLITE_LOCKED := 6. \t\t\t"A table in the database is locked"\r\tSQLITE_NOMEM := 7.\t\t\t\t"A malloc() failed"\r\tSQLITE_READONLY := 8.\t\t\t"Attempt to write a readonly database"\r\tSQLITE_INTERRUPT := 9.\t\t"Operation terminated by sqlite3_interrupt()"\r\tSQLITE_IOERR := 10.\t\t\t"Some kind of disk I/O error occurred"\r\tSQLITE_CORRUPT := 11.\t\t\t"The database disk image is malformed"\r\tSQLITE_NOTFOUND := 12.\t\t"Unknown opcode in sqlite3_file_control()"\r\tSQLITE_FULL := 13.\t\t\t\t"Insertion failed because database is full"\r\tSQLITE_CANTOPEN := 14.   \t"Unable to open the database file"\r\tSQLITE_PROTOCOL := 15.\t\t"Database lock protocol error"\r\tSQLITE_EMPTY := 16.\t\t\t"Database is empty"\r\tSQLITE_SCHEMA := 17.\t\t\t"The database schema changed"\r\tSQLITE_TOOBIG := 18.\t\t\t"String or BLOB exceeds size limit"\r\tSQLITE_CONSTRAINT := 19.\t\t"Abort due to constraint violation"\r\tSQLITE_MISMATCH := 20.\t\t"Data type mismatch"\r\tSQLITE_MISUSE := 21. \t\t\t"Library used incorrectly"\r\tSQLITE_NOLFS := 22.\t\t\t"Uses OS features not supported on host"\r\tSQLITE_AUTH := 23.\t\t\t\t"Authorization denied"\r\tSQLITE_FORMAT := 24.\t\t\t"Auxiliary database format error"\r\tSQLITE_RANGE := 25.\t\t\t"2nd parameter to sqlite3_bind out of range"\r\tSQLITE_NOTADB := 26.\t\t\t"File opened that is not a database file"\r\tSQLITE_NOTICE := 27.\t\t\t"Notifications from sqlite3_log()"\r\tSQLITE_WARNING := 28.\t\t\t"Warnings from sqlite3_log()"',
			#stamp : '',
			#package : #SQLite3-Core
		}
	}
}OmEntry {
